{
  "env": {
    "development": {
      "presets": [
        [
          "next/babel",
          {
            "styled-jsx": {
              "plugins": [
                "styled-jsx-plugin-postcss"
              ]
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            },
            "styled-jsx": {
              "plugins": [
                "styled-jsx-plugin-postcss"
              ]
            }
          }
        ]
      ]
    }
  }
}